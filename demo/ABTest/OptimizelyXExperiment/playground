
const experimentsInfo = {
   40000:{
      id:"8401924730",
      isActive: true,
      isInExperimentHoldback: false,
      variation:{
         id:"400002",
         name:"Variation #2"
      }
   }
}

window.optimizely = {
  get: () => ({ getExperimentStates: () => experimentsInfo })
}

return (<section>
  <h2>Inactive OptimizelyX Experiment</h2>
  <p>When experiment is not active, no variation should be assigned.</p>
  <AbTestOptimizelyXExperiment experimentId={30000}>
    <button variationId={300000} defaultVariation>Variation Default (Expected)</button>
    <button variationId={300001}>Variation A (Not Expected)</button>
    <button variationId={300002}>Variation B (Not Expected)</button>
    <button variationId={300003}>Variation C (Not Expected)</button>
  </AbTestOptimizelyXExperiment>

  <h2>Active OptimizelyX Experiment</h2>
  <p>When experiment is active, it should show the accurate variation.</p>
  <AbTestOptimizelyXExperiment experimentId={40000}>
    <button variationId={400000} defaultVariation>Variation Default (Not Expected)</button>
    <button variationId={400001}>Variation A (Not Expected)</button>
    <button variationId={400002}>Variation B (Expected)</button>
