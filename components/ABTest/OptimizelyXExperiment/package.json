    "sass": "../../../node_modules/.bin/cpx \"./src/**/*.scss\" ./lib",
    "preversion": "echo \"preversion\"",
    "version": "npm run build",
    "postversion": "git add ${PWD} && git commit -m \"release(ABTest/OptimizelyXExperiment): v$(node -p -e \"require('./package.json').version\")\" && git push origin master && npm publish"
  },
